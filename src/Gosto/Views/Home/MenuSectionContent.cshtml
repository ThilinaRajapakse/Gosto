@model Gosto.ViewModels.MenuSectionItemsVM

<table>
    <thead>
        <tr>
            <th>Product</th>
            <th>Price</th>
        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < Model.MenuItems.Count; i++)
        {
            var item = Model.MenuItems[i];
            <tr>
                <td>@item.Name</td>
                <td>@item.Price</td>
            </tr>
        }
    </tbody>
</table>  